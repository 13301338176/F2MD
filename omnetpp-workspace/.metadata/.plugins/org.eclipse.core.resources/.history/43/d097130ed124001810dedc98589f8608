#include "BsmCheck.h"

BsmCheck::BsmCheck() {
     art = 0;
     mgtSpeed = 0;
     mgtDistance = 0;
     maxSpeed = 0;
     inter = 0;
     saw = 0;
     mbf = 0;
     map = 0;
}

double BsmCheck::getART() {
    return art;
}

double BsmCheck::getMGTDistance() {
    return mgtDistance;
}

double BsmCheck::getMGTSpeed() {
    return mgtSpeed;
}

double BsmCheck::getMAXSpeed() {
    return maxSpeed;
}

double BsmCheck::getINTER() {
    return inter;
}

double BsmCheck::getSAW() {
    return saw;
}

double BsmCheck::getMBF() {
    return mbf;
}

double BsmCheck::getMAP() {
    return map;
}

void BsmCheck::setART() {
    this->art;
}

void BsmCheck::setMGTDistance(double mgtDistance) {
    this->mgtDistance = mgtDistance;
}

void BsmCheck::setMGTSpeed() {
    this->mgtSpeed;
}

void BsmCheck::setMAXSpeed() {
    this->maxSpeed;
}

void BsmCheck::setINTER() {
    this->inter;
}

void BsmCheck::setSAW() {
    this->saw;
}

void BsmCheck::setMBF() {
    this->mbf;
}

void BsmCheck::setMAP() {
    this->map;
}



